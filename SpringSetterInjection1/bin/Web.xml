<?xml version="1.0" encoding="UTF-8"?>
<beans 
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans   
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
    
  <bean id="add" class="com.div.Address">
  <property name="addressLine1" value="138"></property>
  <property name="city" value="indore"></property>
  <property name="state" value="mp"></property>
  <property name="country" value="india"></property>
  </bean>
  
  <bean id="emp" class="com.div.Employee">
  <property name="id" value="11"></property>
  <property name="name" value="aarti"></property>
  <property name="address" ref="add"></property>
  </bean>


</beans>